{
	"manifest_version":2,
	"name":"URL QR",
	"description":"Make QR from URL",
	"version":"1.0",
	
	"icons": {
		"16": "img/icon_16.png",
		"32": "img/icon_32.png",
		"48": "img/icon_48.png",
		"64": "img/icon_64.png",
		"128": "img/icon_128.png"
	},

	"browser_action": {
		"default_icon": "img/icon-48.png",
		"default_title": "URL QR",
		"default_popup": "popup.html"
	},

	"permissions": [
		"tabs"
	]
}
